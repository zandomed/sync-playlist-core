meta {
  name: Google OAuth2
  type: http
  seq: 3
}

get {
  url: {{URL}}/auth/oauth2/google
  body: none
  auth: none
}

tests {
  test("should return OAuth2 URL for Google", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody().authUrl).to.be.a('string');
    expect(res.getBody().authUrl).to.contain('/oauth2/authorization/google');
    expect(res.getBody().provider).to.equal('google');
  });
}